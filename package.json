{
  "name": "willcodeforbeer",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "data": "ts-node ./scripts/find_til.ts",
    "data:watch": "ts-node ./scripts/watch_til.ts",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -i ./app/styles/style.css -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve build",
    "test": "vitest",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "4.2.1",
    "@remix-run/node": "1.7.0",
    "@remix-run/react": "1.7.0",
    "@remix-run/serve": "1.7.0",
    "@tailwindcss/typography": "0.5.4",
    "@types/ua-parser-js": "0.7.36",
    "clsx": "1.2.1",
    "front-matter": "4.0.2",
    "markdown-it": "13.0.1",
    "markdown-it-anchor": "8.6.4",
    "markdown-it-highlightjs": "4.0.1",
    "markdown-it-link-attributes": "4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "2.1.13",
    "tiny-invariant": "^1.2.0",
    "ua-parser-js": "1.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.7.0",
    "@remix-run/eslint-config": "1.7.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/eslint": "8.4.6",
    "@types/markdown-it-highlightjs": "3.3.1",
    "@types/markdown-it-link-attributes": "3.0.1",
    "@types/marked": "4.0.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/recharts": "1.8.23",
    "@vitejs/plugin-react": "2.0.1",
    "c8": "7.12.0",
    "chokidar": "3.5.3",
    "cross-env": "^7.0.3",
    "esbuild-register": "3.3.3",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "6.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "prisma": "4.2.1",
    "tailwindcss": "3.1.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.2",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.22.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
}
