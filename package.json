{
  "name": "willcodeforbeer",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "data": "ts-node ./scripts/find_til.ts",
    "data:watch": "ts-node ./scripts/watch_til.ts",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -i ./app/styles/style.css -o ./app/styles/tailwind.css",
    "postinstall": "remix setup node",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve build",
    "test": "vitest",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "3.14.0",
    "@remix-run/react": "1.5.1",
    "@remix-run/serve": "1.5.1",
    "@tailwindcss/typography": "0.5.2",
    "clsx": "1.1.1",
    "front-matter": "4.0.2",
    "markdown-it": "13.0.1",
    "markdown-it-anchor": "8.6.4",
    "markdown-it-highlightjs": "4.0.1",
    "markdown-it-link-attributes": "4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recharts": "2.1.10",
    "remix": "1.5.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.5.1",
    "@remix-run/eslint-config": "1.5.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/eslint": "8.4.2",
    "@types/markdown-it-highlightjs": "3.3.1",
    "@types/markdown-it-link-attributes": "3.0.1",
    "@types/marked": "4.0.3",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "@vitejs/plugin-react": "1.3.2",
    "c8": "7.11.3",
    "chokidar": "3.5.3",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.3.2",
    "eslint": "8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.11",
    "prisma": "3.14.0",
    "tailwindcss": "3.0.24",
    "ts-node": "10.8.0",
    "typescript": "4.7.2",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.13.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
}
